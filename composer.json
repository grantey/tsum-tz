{
    "name": "grantey/tsum-tz",
    "license": "MIT",
    "type": "project",
    "description": "warehouse",
    "version": "1.0",
    "require": {
        "php": "^7.1.3",
        "php-activerecord/php-activerecord": "dev-master",
        "phpauth/phpauth": "dev-master",
        "twig/twig": "^2.4"
    },
    "require-dev": {
    },
    "repositories": [
	    {
	    	"type": "git",
	    	"url": "https://github.com/jpfuentes2/php-activerecord"
	    },
	    {
	    	"type": "git",
	    	"url": "https://github.com/PHPAuth/PHPAuth.git"
	    }
	],
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Migrations\\": "src/migrations",
            "App\\Warehouse\\": "src/warehouse"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
